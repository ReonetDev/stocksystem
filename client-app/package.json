{
  "name": "reonetstock",
  "productName": "Reonet Stock",
  "description": "Reonet Stock Control System",
  "author": "Reonet (PTY) LTD",
  "private": true,
  "version": "1.0.7",
  "copyright": "2025 Â©",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && NODE_ENV=development electron .\"",
    "package": "electron-builder --mac --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.stockcontrolsystem.app",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "splash.html",
      "splash-preload.js",
      "reoicon.png"
    ],
    "icon": "reoicon.png",
    "directories": {
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "ReonetDev",
      "repo": "stocksystem"
    },
    "artifactName": "Reonet.Stock.Setup.${version}.${ext}",
    "win": {
      "target": [
        "nsis:x64"
      ],
      "extraResources": [
        {
          "from": "api/win-x64/",
          "to": "api/win-x64/"
        },
        {
          "from": "dist/assets",
          "to": "assets"
        }
      ]
    },
    "mac": {
      "extraResources": [
        {
          "from": "api/osx-arm64/",
          "to": "api/osx-arm64/"
        },
        {
          "from": "dist/assets",
          "to": "assets"
        }
      ]
    }
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.13.4",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.2.0",
    "bootstrap": "^5.3.7",
    "electron-updater": "^6.6.2",
    "file-saver": "^2.0.5",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.6.3",
    "react-to-print": "^3.1.1",
    "react-toastify": "^10.0.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.3",
    "wait-on": "^8.0.3"
  }
}
